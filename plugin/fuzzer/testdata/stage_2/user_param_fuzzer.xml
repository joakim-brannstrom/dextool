<dextool>
    <symbols>
        <symbol name="band_limit">
            <param name="v">
                <fuzz use="dextool::fuzz_r" param="2000, 4000" include="stdint.h"/>
            </param>
        </symbol>
        <symbol name="lower_limit">
            <param name="v">
                <fuzz use="dextool::fuzz_r" param="1000, 5000"/>
            </param>
        </symbol>
        <symbol name="nolimit">
            <param name="v">
                <fuzz use=""/>
            </param>
        </symbol>
        <symbol name="upper_limit">
            <param name="v">
                <fuzz use="dextool::fuzz_r" param="-1000, 1000"/>
            </param>
        </symbol>
        <symbol name="this_is_removed" filter="exclude" />
    </symbols>
</dextool>
