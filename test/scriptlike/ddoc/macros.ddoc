Ddoc

Macros:
	H2 = <h2>$0</h2>
	BR = <br />
	DOLLAR = $
	COLON = &#58;
	EM       = $(B $(I $0) )
	ISSUE    = $(LINK2 https://github.com/Abscissa/scriptlike/issues/$0, #$0)
	ENHANCE  = $(LI $(B Enhancement$(COLON)) $0)
	CHANGE   = $(LI $(B Change$(COLON)) $0)
	FIXED    = $(LI $(B Fixed$(COLON)) $0)
	API_BASE = $(DDOX_ROOT_DIR)scriptlike/
	API_CORE      = $(D_INLINECODE $(LINK2 $(API_BASE)core/$0.html, $0))
	API_FAIL      = $(D_INLINECODE $(LINK2 $(API_BASE)fail/$0.html, $0))
	API_FILE_EXTR = $(D_INLINECODE $(LINK2 $(API_BASE)file/extras/$0.html, $0))
	API_FILE_WRAP = $(D_INLINECODE $(LINK2 $(API_BASE)file/wrappers/$0.html, $0))
	API_INTERACT  = $(D_INLINECODE $(LINK2 $(API_BASE)interact/$0.html, $0))
	API_PATH_EXTR = $(D_INLINECODE $(LINK2 $(API_BASE)path/extras/$0.html, $0))
	API_PATH_WRAP = $(D_INLINECODE $(LINK2 $(API_BASE)path/wrappers/$0.html, $0))
	API_PROCESS   = $(D_INLINECODE $(LINK2 $(API_BASE)process/$0.html, $0))
	MODULE_CORE      = $(D_INLINECODE $(LINK2 $(API_BASE)core.html, scriptlike.core))
	MODULE_FAIL      = $(D_INLINECODE $(LINK2 $(API_BASE)fail.html, scriptlike.fail))
	MODULE_FILE      = $(D_INLINECODE $(LINK2 $(API_BASE)file.html, scriptlike.file))
	MODULE_FILE_EXTR = $(D_INLINECODE $(LINK2 $(API_BASE)file/extras.html, scriptlike.file.extras))
	MODULE_FILE_WRAP = $(D_INLINECODE $(LINK2 $(API_BASE)file/wrappers.html, scriptlike.file.wrappers))
	MODULE_INTERACT  = $(D_INLINECODE $(LINK2 $(API_BASE)interact.html, scriptlike.interact))
	MODULE_PATH      = $(D_INLINECODE $(LINK2 $(API_BASE)path.html, scriptlike.path))
	MODULE_PATH_EXTR = $(D_INLINECODE $(LINK2 $(API_BASE)path/extras.html, scriptlike.path.extras))
	MODULE_PATH_WRAP = $(D_INLINECODE $(LINK2 $(API_BASE)path/wrappers.html, scriptlike.path.wrappers))
	MODULE_PROCESS   = $(D_INLINECODE $(LINK2 $(API_BASE)process.html, scriptlike.process))
	STD_EXCEPTION = $(D_INLINECODE $(LINK2 http://dlang.org/phobos/std_exception.html#$0, $0))
	STD_FILE      = $(D_INLINECODE $(LINK2 http://dlang.org/phobos/std_file.html#$0, $0))
	STD_PATH      = $(D_INLINECODE $(LINK2 http://dlang.org/phobos/std_path.html#$0, $0))
	STD_PROCESS   = $(D_INLINECODE $(LINK2 http://dlang.org/phobos/std_process.html#$0, $0))
	STD_STDIO     = $(D_INLINECODE $(LINK2 http://dlang.org/phobos/std_stdio.html#$0, $0))
	FULL_STD_EXCEPTION = $(D_INLINECODE $(LINK2 http://dlang.org/phobos/std_exception.html#$0, std.exception.$0))
	FULL_STD_FILE      = $(D_INLINECODE $(LINK2 http://dlang.org/phobos/std_file.html#$0, std.file.$0))
	FULL_STD_PATH      = $(D_INLINECODE $(LINK2 http://dlang.org/phobos/std_path.html#$0, std.path.$0))
	FULL_STD_PROCESS   = $(D_INLINECODE $(LINK2 http://dlang.org/phobos/std_process.html#$0, std.process.$0))
	FULL_STD_STDIO     = $(D_INLINECODE $(LINK2 http://dlang.org/phobos/std_stdio.html#$0, std.stdio.$0))
	DMD             = $(LINK2 http://dlang.org/, DMD)
	DDOX            = $(LINK2 https://github.com/rejectedsoftware/ddox, ddox)
	MODULE_STD_FILE = $(D_INLINECODE $(LINK2 http://dlang.org/phobos/std_file.html, std.file))
	MODULE_STD_PATH = $(D_INLINECODE $(LINK2 http://dlang.org/phobos/std_path.html, std.path))
	LICENSE = $(LINK2 https://github.com/Abscissa/scriptlike/blob/master/LICENSE.txt, zlib/libpng)
